#include<iostream>
using namespace std;
int smallestelement(int arr[],int n){
int min=arr[0];
for(int i=0;i<n;i++){
    if (min >arr[i]){
        min = arr[i];
    }
}
return min;
}
int main(){
int array1[]={1,3,68,46,65,37};
int n =6;
int min=smallestelement(array1,n);
cout<<"the smallestelement in the array is:"<<min<<endl;

int array2[]={2,5,4,3};
 n=4;
min=smallestelement(array2,n);

cout<<"the smallestelement in the array is:"<<min;
return 0;
}
