#include<iostream>
using namespace std;
void reverse (int arr[],int n){
    int start=0;
    int end= n-1;
    while(start<=end){
        swap(arr[start],arr[end]);
        start++;
        end--;
    }
}
 void printarray(int arr[],int n){

for(int i=0;i<n;i++){
    cout<<arr[i]<<" ";
}
cout<<endl;
}
int main(){
    int arri[6]={2,6,4,36,33,36};
    int arrii[8]={23,4,22,6,3,2,22,425};
    reverse(arri,6);
    reverse(arrii,8);
    printarray(arri,6);
    printarray(arrii,8);
}
