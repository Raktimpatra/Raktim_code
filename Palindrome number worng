#include<iostream>
using namespace std;
int reverse{
int num;
cout<<"enter the number ";
cin>>num;
if(num<0){
    cout<<"not palindrome"<<endl;
}
while(num!=0){
    int digt= num%10;
    rev=(rev*10)+digit;;
    num=num/10;
}
return rev;
}
int main(){
if(num==rev){
    cout<<"palindrome";
}
else{
cout<<"not palindrome";
}
return 0;
}
