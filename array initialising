#include<iostream>
using namespace std;
void printarray(){
}
int main(){
    //declare
    int number [15];
// accessing an arry
cout << "index no of 14 value" << number[14]<<endl;

// intialising an array 
int second[3]={14,18,20};
//accessingan element
cout << "value at 0 index   "<< second[0]<< endl;
cout << "value at 2 index   "<< second[2]<< endl;




int third[16]={2,8};
int n=16;
cout<<"print the array is  ";

for(int i=0; i<n; i++){
    cout<< third[i] <<" ";
    
}
int fourth[14]={1};
n=14;
cout<<"print the array is  ";

for(int i=0; i<n; i++){
    cout<< fourth[i] <<" ";
}
}
